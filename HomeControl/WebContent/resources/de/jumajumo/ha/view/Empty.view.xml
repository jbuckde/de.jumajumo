<mvc:View controllerName="de.jumajumo.ha.view.HomeAutomationObjectList"
	xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns:html="http://www.w3.org/1999/xhtml">

	    <TileContainer>
	        <StandardTile title="Garage"
						icon="sap-icon://car-rental"
						iconDensityAware="true"
						press="navToGarage"></StandardTile>
	        <StandardTile title="Cameras"
						icon="sap-icon://video"
						iconDensityAware="true"
						press="navToCamera"></StandardTile>
	        <StandardTile title="Configuration"
						icon="sap-icon://settings"
						iconDensityAware="true"
						press="navToConfiguration"></StandardTile>
	    </TileContainer>

</mvc:View>